local redzlib = loadstring(game:HttpGet("https://raw.githubusercontent.com/REDzHUB/RedzLibV5/main/Source.Lua"))()

local Window = redzlib:MakeWindow({
  Title = "ريك",
  SubTitle = "سكربت",
  SaveFolder = "Redz Config"
})

local Tab1 = Window:MakeTab({"القائمه الاساسيه", "cool"})

Tab1:AddButton({"نسخ رابط قناه التليكرام", function()
setclipboard('https://t.me/Hacatpeter')
end})

Tab1:AddButton({" نسخ يوزر المروج ", function()
setclipboard('@PE_T9')
end})

Tab1:AddButton({" نسخ يوزر ممول السكربتات  ", function()
setclipboard('@salih7711')
end})

Tab1:AddButton({"نسخ يوزر صانع السكربت", function()
setclipboard('@AP_7N')
end})
local Toggle1 = Tab1:AddToggle({
  Name = "Speed",
  Description = "Idk",
  Default = false
})

Tab1:AddSlider({
  Name = "سرعه اللاعب",
  Min = 1,
  Max = 100,
  Increase = 1,
  Default = 16,
  Callback = function(Value)
  game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
  end
})


local Tab1 = Window:MakeTab({"بلوكس فروت", "cool"})


